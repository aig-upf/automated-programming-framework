		( AIRPLANE ?0 )
		( AIRPORT ?1 )
		( NOT ( LOCKED-AT ?0 ?1 ) )
	)
	( 	( !!LOCK-AT  ?0 ?1 ) )
)
( :METHOD (ACHIEVE-AT ( ?0 ?1 )
)
	( AND
		( NOT ( ACHIEVING-AT1 ?0 ?1 ) )
		( NOT ( ACHIEVING-AT2 ?0 ?1 ) )
		( NOT ( ACHIEVING-AT0 ?0 ?1 ) )
		( NOT ( AT ?0 ?1 ) )
		( AIRPLANE ?0 )
		( AIRPORT ?1 )
		( NOT ( LOCKED-AT ?0 ?1 ) )
	)
	( 	( !!ACHIEVE2  ?0 )	( ACHIEVE-AT2  ?0 ?1 )	( !!LOCK-AT  ?0 ?1 )	( !!STOPALL2  ?0 ) )
)
( :METHOD (ACHIEVE-AT2 ( ?0 ?1 )
)
	( AND
		( AT ?0 ?1 )
		( AIRPLANE ?0 )
		( AIRPORT ?1 )
	)
	()
)
( :METHOD (ACHIEVE-AT2 ( ?0 ?1 )
)
	( AND
		( NOT ( AT ?0 ?1 ) )
		( AT ?0 ?2 )
		( AIRPLANE ?0 )
		( AIRPORT ?2 )
		( AIRPORT ?1 )
	)
	( 	( !FLY-AIRPLANE  ?0 ?2 ?1 )	( ACHIEVE-AT2  ?0 ?1 ) )
)
)
